{
  "name": "twitch-data",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "immer": "^1.5.0",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4",
    "react-vis": "^1.10.4"
  },
  "scripts": {
